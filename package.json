{
    "name": "ions-highlighter",
    "displayName": "IonS Highlighter",
    "publisher": "Skulhunter5",
    "description": "Syntax highlighter for IonS",
    "version": "0.0.4",
    "engines": {
        "vscode": "^1.63.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "ions",
            "aliases": ["IonS", "ions"],
            "extensions": [".ions"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "ions",
            "scopeName": "source.ions",
            "path": "./syntaxes/ions.tmLanguage.json"
        }]
    }
}